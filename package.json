{
	"name": "crate-finder",
	"displayName": "Crate Finder",
  "description": "Find the crates you're looking for without leaving your editor!",
  "publisher": "wikiwong",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/wikiwong/crate-finder.git"
  },
	"engines": {
		"vscode": "^1.36.0"
  },
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:findCrate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "findCrate",
				"title": "Find Crate"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"axios": "^0.19.0"
	}
}
